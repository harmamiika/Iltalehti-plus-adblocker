{
  "manifest_version": 3,
  "name": "'Iltalehti+'-Adblocker",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "description": "Blocks 'Iltalehti+'-ads in iltalehti.fi",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
