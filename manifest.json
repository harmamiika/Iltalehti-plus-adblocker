{
  "manifest_version": 3,
  "name": "Iltalehti Adblocker",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "description": "Blocks ads on iltalehti.fi",
  "content_scripts": [
    {
      "matches": ["https://www.iltalehti.fi/*"],
      "js": ["index.js"]
    }
  ],
  "permissions": ["activeTab"]
}
